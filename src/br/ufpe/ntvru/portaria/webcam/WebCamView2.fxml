                  <Insets right="-20.0" />
               </BorderPane.margin>
            </ComboBox>
         </center>
      </BorderPane>
      <ImageView fx:id="visitorsCameraImgView" fitHeight="308.0" fitWidth="332.0" layoutX="111.0" layoutY="63.0"  pickOnBounds="true" preserveRatio="true">

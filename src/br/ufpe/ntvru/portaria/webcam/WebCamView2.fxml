<AnchorPane fx:id="visitorsAPanePicture" prefHeight="459.0" prefWidth="529.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.ufpe.ntvru.portaria.webcam.WebCamController">
   <children>
